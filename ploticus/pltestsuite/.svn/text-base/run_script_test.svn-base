#!/bin/sh
#
PL="pl"
OUT=Diag.out

which $PL
echo "Enter an output format, one of: x11, gif, png, svg, (svgz), (jpeg), or eps: \c"
read MODE

echo "Diagnostic output will be placed into file $OUT.  Press Enter to begin..\c"
read X


which $PL > $OUT
date >> $OUT

OPTS=

echo "--------- stock2..." >> $OUT
$PL -$MODE $OPTS  stock2.htm 2>> $OUT
echo "--------- timely..." >> $OUT
$PL -$MODE $OPTS  timely.htm 2>> $OUT
echo "Note: some numbers should have appeared above..
" >> $OUT

echo "--------- clickmap_mouse... (should produce a server-side image map file clickmap_mouse.map ..)" >> $OUT
$PL -$MODE $OPTS clickmap_mouse.htm -map -mapfile mouse.map >> $OUT
wc -l mouse.map >> $OUT

echo "--------- kmslide..." >> $OUT
$PL -$MODE $OPTS  kmslide.htm 2>> $OUT
echo "--------- propbars1..." >> $OUT
$PL -$MODE $OPTS  propbars1.htm 2>> $OUT
