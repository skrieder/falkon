  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/InMemoryServiceGroupEntry_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/InMemoryServiceGroupEntry_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/InMemoryServiceGroup_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/InMemoryServiceGroup_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/inmemory_service_group_entry_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/inmemorysg/inmemory_service_group_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/mds/servicegroup/ServiceGroupFactory_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/mds/servicegroup/ServiceGroupFactory_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/mds/servicegroup/factory_port_type.wsdl"/>
</target>
</project>
