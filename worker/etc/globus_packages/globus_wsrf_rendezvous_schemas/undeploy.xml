<project default="undeploy" basedir=".">


<property environment="env"/>
<property file="build.properties"/>
<property file="${user.home}/build.properties"/>
<property name="env.GLOBUS_LOCATION" value="../../.."/>
<property name="deploy.dir" value="${env.GLOBUS_LOCATION}"/>


<target name="undeploy">
  <delete verbose="true" dir="${deploy.dir}/etc/globus_wsrf_rendezvous_schemas"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_data.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_data.xsd"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_factory_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_factory_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_factory_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/rendezvous/rendezvous_types.xsd"/>
</target>
</project>
