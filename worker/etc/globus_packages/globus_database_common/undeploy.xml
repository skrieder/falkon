<project default="undeploy" basedir=".">


<property environment="env"/>
<property file="build.properties"/>
<property file="${user.home}/build.properties"/>
<property name="env.GLOBUS_LOCATION" value="../../.."/>
<property name="deploy.dir" value="${env.GLOBUS_LOCATION}"/>


<target name="undeploy">
  <delete verbose="true" dir="${deploy.dir}/etc/globus_database_common"/>
  <delete verbose="true" file="${deploy.dir}/lib/commons-dbcp.jar"/>
  <delete verbose="true" file="${deploy.dir}/lib/commons-pool.jar"/>
  <delete verbose="true" file="${deploy.dir}/share/licenses/commons.LICENSE"/>
  <delete verbose="true" file="${deploy.dir}/share/licenses/pg73jdbc2.LICENSE"/>
  <delete verbose="true" file="${deploy.dir}/lib/pg73jdbc2.jar"/>
</target>
</project>
