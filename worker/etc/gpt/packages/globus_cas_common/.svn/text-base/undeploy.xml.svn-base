  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_admin_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_bindings.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_faults.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_flattened.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_query_port_type.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_service.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/cas/cas_types.xsd"/>
</target>
</project>
