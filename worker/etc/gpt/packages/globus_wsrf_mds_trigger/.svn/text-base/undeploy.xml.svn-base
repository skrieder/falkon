<project default="undeploy" basedir=".">


<property environment="env"/>
<property file="build.properties"/>
<property file="${user.home}/build.properties"/>
<property name="env.GLOBUS_LOCATION" value="../../.."/>
<property name="deploy.dir" value="${env.GLOBUS_LOCATION}"/>


<target name="undeploy">
  <delete verbose="true" dir="${deploy.dir}/etc/globus_wsrf_mds_trigger"/>
  <delete verbose="true" file="${deploy.dir}/lib/wsrf_mds_trigger.jar"/>
  <delete verbose="true" file="${deploy.dir}/lib/wsrf_mds_trigger_stubs.jar"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/mds/trigger/trigger-types.wsdl"/>
  <delete verbose="true" file="${deploy.dir}/share/schema/mds/trigger/trigger-types.xsd"/>
</target>
</project>
